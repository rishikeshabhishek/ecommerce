<%- include("partial/header") %>
    <%- include("partial/nav") %>
        <%- include("partial/banner") %>
            <%- include("partial/feature") %>

                <!-- start product Area -->

                <!-- single product slide -->
                <div class="single-product-slider">
                    <div class="container">
                        <div class="row justify-content-center">
                            <div class="col-lg-6 text-center">
                                <div class="section-title">
                                    <h1>All Latest Products</h1>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <!-- single product -->
                            <% for(data of displayData) {%>
                                <div class="col-lg-3 col-md-6">
                                    <div class="single-product">
                                        <a href="view-single-product/<%- data._id %>"><img class="img-fluid productimg" src="uploads/<%- data.image %>" alt=""></a>
                                        <div class="product-details">
                                            <h6>
                                                <%- data.productName %>
                                            </h6>
                                            <div class="price">
                                                <h6>
                                                    &#8377;
                                                    <%- data.price %>
                                                </h6>
                                                <h6 class="l-through">&#8377;
                                                    <%- data.price+150 %>
                                                </h6>
                                            </div>
                                            <div class="prd-bottom">

                                                <a href="add-to-bag/<%- data._id %>" class="social-info">
                                                    <span class="ti-bag"></span>
                                                    <p class="hover-text">add to bag</p>
                                                </a>
                                                <!-- <a href="" class="social-info">
                                                    <span class="lnr lnr-heart"></span>
                                                    <p class="hover-text">Wishlist</p>
                                                </a>
                                                <a href="" class="social-info">
                                                    <span class="lnr lnr-sync"></span>
                                                    <p class="hover-text">compare</p>
                                                </a>
                                                <a href="" class="social-info">
                                                    <span class="lnr lnr-move"></span>
                                                    <p class="hover-text">view more</p>
                                                </a> -->
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                        </div>
                    </div>
                </div>
                <%- include("partial/brand") %>

                    <%- include("partial/footer") %>